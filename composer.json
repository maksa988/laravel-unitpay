{
    "name": "maksa988/laravel-unitpay",
    "description": "UnitPay payments for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "maksa988",
            "email": "maksa988ua@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "^5.3.10",
        "guzzlehttp/guzzle": "~6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "orchestra/testbench": "^3.3.3"
    },
    "autoload": {
        "psr-4": {
            "Maksa988\\UnitPay\\Facades\\": "src/Facades",
            "Maksa988\\UnitPay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maksa988\\UnitPay\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Maksa988\\UnitPay\\UnitPayServiceProvider"
            ],
            "aliases": {
                "UnitPay": "Maksa988\\UnitPay\\Facades\\UnitPay"
            }
        }
    }
}
